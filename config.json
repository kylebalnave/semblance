{
   "classpaths" : [
       "../affirmation/dist/",
       "../disparity/dist/",
       "../snapshot/dist/"
   ],
   "dev":[

   ],
   "dist":{
      "runners":[
         {
            "className":"snapshot.runners.SnapshotRunner",
            "out":"./test/snaps/",
            "urls":[
               "http://www.yahoo.com/"
            ],
            "dimensions":[
               [
                  1600,
                  1200
               ]
            ],
            "drivers":[
               {
                  "name":"phantomjs",
                  "version":"",
                  "hub":""
               }
            ],
            "reports":[
               {
                  "className":"semblance.reporters.JunitReport",
                  "out":"./test/reports/snapshot.junit"
               }
            ]
         },
         {
            "className":"disparity.runners.DisparityRunner",
            "in":"./test/snaps/",
            "out":"./test/snaps/",
            "fuzzyness":10,
            "reports":[
               {
                  "className":"semblance.reporters.JunitReport",
                  "out":"./test/reports/disparity.junit"
               }
            ]
         },
         {
            "className":"affirmation.runners.AffirmationRunner",
            "w3cServiceUrl":"http://validator.w3.org/check",
            "urls":[
               "http://www.yahoo.com/"
            ],
            "ignore":[
               "Syntax of list of link-type keywords",
               "Mistakes that can cause this error include"
            ],
            "reports":[
               {
                  "className":"semblance.reporters.JunitReport",
                  "out":"./test/reports/affirmation.junit"
               },
               {
                  "className":"semblance.reporters.XmlReport",
                  "out":"./test/reports/affirmation.xml"
               },
               {
                  "className":"semblance.reporters.HtmlReport",
                  "out":"./test/reports/affirmation.html"
               },
               {
                  "className":"semblance.reporters.TextReport",
                  "out":"./test/reports/affirmation.txt"
               }
            ]
         }
      ]
   }
}